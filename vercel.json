{
    "version": 2,
    "builds": [
        {
            "src": "./index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
        
            "headers": [
                {
                  "key": "access-control-allow-origin",
                  "value": "*"
                }
            ]
        }
    ]
    
}
